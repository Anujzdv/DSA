#include <bits/stdc++.h>
using namespace std;
int evenNumberBitwiseORs(vector<int>& nums) {
        int res = 0;
        for (int i = 0; i < nums.size(); i++) {
            if (nums[i] % 2 == 0) {
                res = res | nums[i];  // use bitwise OR, not logical OR
            }
        }
        return res;
}
int main(){
  int n ;
  cout << " enter size" ;
  cin>>n;
  vector<int> nums(n);
  cout<<"enter vetor element";
  for(int i = 0; i< n; i++){
    cin>>nums[i];
   }
   cout << evenNumberBitwiseORs(nums);
   return 0;
  }
